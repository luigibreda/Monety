{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "prisma/schema.prisma", 
        "use": "@prisma/cli",
        "config": { "prerun": { "npm": "run prisma:generate" } }
      }
    ],
    "routes": [
      {
        "src": "/v1/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
 